{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "beyondcode/laravel-server-timing": "^1.3",
        "calebporzio/sushi": "^2.3",
        "davidhsianturi/blade-bootstrap-icons": "^1.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "glhd/gretel": "^1.4",
        "guzzlehttp/guzzle": "^7.0.1",
        "jantinnerezo/livewire-alert": "^2.1",
        "kriswallsmith/buzz": "^1.2",
        "laravel/framework": "^8.40",
        "laravel/horizon": "^5.7",
        "laravel/jetstream": "^2.3",
        "laravel/sanctum": "^2.6",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "laudis/neo4j-php-client": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "leanadmin/console-log": "^0.1.1",
        "livewire-ui/modal": "^1.0",
        "livewire/livewire": "^2.0",
        "nyholm/psr7": "^1.4",
        "nyholm/psr7-server": "^1.0",
        "predis/predis": "^1.1",
        "socialiteproviders/microsoft-azure": "^4.2",
        "spatie/laravel-collection-macros": "^7.2",
        "spatie/laravel-permission": "^4.0"
    },
    "require-dev": {
        "accentinteractive/laravel-logcleaner": "^1.0",
        "barryvdh/laravel-debugbar": "^3.6",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laracasts/cypress": "^1.4",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
